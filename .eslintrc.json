{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "react/jsx-curly-brace-presence": "error",
    "import/order": [
      "error",
      {
        "groups": [
          "index",
          "sibling",
          "parent",
          "internal",
          "external",
          "builtin",
          "object",
          "type"
        ]
      }
    ]
  }
}
